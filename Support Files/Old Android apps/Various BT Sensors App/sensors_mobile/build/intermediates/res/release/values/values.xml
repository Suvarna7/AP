    <style name="AppBaseTheme" parent="android:Theme.Holo.Light.DarkActionBar">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowBackground">@drawable/app_background</item>
    </style>
    <style name="NotificationText" parent="android:TextAppearance.StatusBar.EventContent"/>
    <style name="NotificationTitle" parent="android:TextAppearance.StatusBar.EventContent.Title"/>
    <style name="StripedProgressBarHorizontal" parent="android:Widget.Holo.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/striped_progress</item>
        <item name="android:minHeight">10dip</item>
        <item name="android:maxHeight">25dip</item>
    </style>
    <style name="basicText">
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">@color/basic_orange</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="blueText" parent="basicText">
        <item name="android:textColor">@android:color/holo_blue_dark</item>
        <item name="android:gravity">center|left</item>
    </style>
    <style name="defaultLayout">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
